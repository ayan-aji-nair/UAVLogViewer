# UAV Log Viewer Production Environment Variables
# Copy this file to .env and fill in your values for production deployment

# Required: OpenAI API Key for the AI chatbot
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_api_key_here

# Optional: OpenAI Model (default: gpt-4)
# Options: gpt-4, gpt-4-turbo, gpt-3.5-turbo
OPENAI_MODEL=gpt-4

# Optional: Cesium Token for 3D maps
# Get a free token from: https://cesium.com/ion/signup/
VUE_APP_CESIUM_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_cesium_token_here

# Production Settings
DEBUG=false
LOG_LEVEL=info

# Security Settings
REDIS_PASSWORD=your_secure_redis_password_here

# CORS Settings (comma-separated list of allowed origins)
CORS_ORIGINS=["https://yourdomain.com", "https://www.yourdomain.com"]

# API URL for frontend (should match your domain)
API_URL=https://yourdomain.com/api

# Optional: SSL Certificate Paths (for HTTPS)
# SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
# SSL_KEY_PATH=/etc/nginx/ssl/key.pem 